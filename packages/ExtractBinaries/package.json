{
  "name": "n8n-nodes-extract-binaries",
  "version": "0.0.3",
  "description": "Extract binary files from the input node in n8n.",
  "keywords": [
    "n8n-community-node-package",
    "n8n-extract-binaries"
  ],
  "homepage": "https://github.com/melvinotieno/n8n-nodes/tree/main/packages/ExtractBinaries",
  "bugs": "https://github.com/melvinotieno/n8n-nodes/issues",
  "license": "MIT",
  "author": "Melvin Otieno <o.melvinotieno@gmail.com> (https://melvinotieno.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/melvinotieno/n8n-nodes/tree/main/packages/ExtractBinaries"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "pnpm rimraf dist && tsc && gulp copy:assets",
